{
  "name": "react-vapor",
  "version": "0.0.0",
  "description": "Vapor CSS components implemented with React!",
  "homepage": "https://github.com/JPLachance/react-vapor#readme",
  "author": "Coveo",
  "license": "MIT",
  "main": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JPLachance/react-vapor.git"
  },
  "scripts": {
    "dev": "webpack-dev-server --devtool eval --inline --hot --progress --colors",
    "build": "NODE_ENV=production webpack --config webpack.prod.config.js",
    "prebuild": "rm -rf dist && mkdir dist",
    "prepublish": "npm run build",
    "postbuild": "NODE_ENV=production TARGET=minify webpack --config webpack.prod.config.js",
    "postinstall": "typings install",
    "add-file": "tsconfig . -i 2"
  },
  "dependencies": {
    "react-tether": "0.5.2"
  },
  "devDependencies": {
    "autoprefixer-loader": "3.2.0",
    "backbone": "1.3.3",
    "coveo-styleguide": "1.4.25",
    "css-loader": "0.25.0",
    "gulp": "3.9.1",
    "gulp-help": "1.6.1",
    "jquery": "3.1.0",
    "node-sass": "3.10.0",
    "pretty-typescript": "1.13.0",
    "react": "15.3.1",
    "react-dom": "15.3.1",
    "react-redux": "4.4.5",
    "redux": "3.6.0",
    "sass-loader": "4.0.2",
    "style-loader": "0.13.1",
    "ts-loader": "0.8.2",
    "tsconfig-glob": "0.4.3",
    "tslint": "3.15.1",
    "tslint-loader": "2.1.5",
    "typescript": "1.8.10",
    "underscore": "1.8.3",
    "webpack": "1.13.2",
    "webpack-dev-server": "1.15.2"
  }
}
