{
  "compilerOptions": {
    "removeComments": true,
    "target": "ES5",
    "module": "commonjs",
    "outDir": "./dist/split/",
    "noEmitOnError": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "jsx": "react"
